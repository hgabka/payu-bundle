{
    "name": "hgabka/payu-bundle",
    "type": "payu-bundle",
    "description": "PayU bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/hgabka/payu-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "hgabka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "symfony/symfony": "~3.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "classmap": [
            "vendor/hgabka/payu-bundle/sdk/"
        ],
        "psr-4": {
            "Hgabka\\PayUBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hgabka\\PayUBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}